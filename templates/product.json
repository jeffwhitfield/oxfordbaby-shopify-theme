    "56c0f5bf-a379-4f47-8c8a-684488062589": {
      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "{% for collection in product.collections %}\n  {% if collection.title contains 'Collection' %}\n    {% assign current_collection = collection.title %}\n  {% endif %}\n{% endfor %}\n\n<div class=\"color-background-1 isolate gradient\">\n  <div class=\"collection section-{{ section.id }}-padding\">\n    <div class=\"collection__title title-wrapper title-wrapper--no-top-margin page-width\">\n      <h2 class=\"title inline-richtext\">Other Items In {{ current_collection }}<\/h2>\n    <\/div>\n\n    <slider-component class=\"slider-mobile-gutter page-width\">\n      <ul\n        id=\"Slider-{{ section.id }}\"\n        class=\"grid product-grid grid--4-col-desktop grid--2-col-tablet-down\"\n        role=\"list\"\n        aria-label=\"{{ 'general.slider.name' | t }}\"\n      >\n        {% for related in collections.all.products %}\n          {% for collection in related.collections %}\n            <p>{{ collection.title }}<\/p>\n            {% if collection.title == current_collection and product.title != related.title %}\n              <li\n                id=\"Slide-{{ section.id }}-{{ forloop.index }}\"\n                class=\"grid__item\"\n              >\n                {% render 'card-product',\n                  card_product: related,\n                  media_aspect_ratio: section.settings.image_ratio,\n                  image_shape: section.settings.image_shape,\n                  show_secondary_image: section.settings.show_secondary_image,\n                  show_vendor: section.settings.show_vendor,\n                  show_rating: section.settings.show_rating,\n                  show_quick_add: section.settings.enable_quick_add,\n                  section_id: section.id\n                %}\n              <\/li>\n            {% endif %}\n          {% endfor %}\n        {% endfor %}\n      <\/ul>\n    <\/slider-component>\n  <\/div>\n<\/div>",
        "color_scheme": "",
        "padding_top": 40,
        "padding_bottom": 52
      }
